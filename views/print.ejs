<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>POS</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
  </head>
  <body class="bg-dark-subtle">
    <nav class="navbar bg-dark-subtle">
      <div class="container-fluid d-flex justify-content-between p-5 py-2">
        <a class="navbar-brand" href="/home">Hashmi Grain & Bakers</a>
      </div>
    </nav>
    <div class="container">
      <h3>Shop# 9B CANAL HEIGHTS AA BLOCK SEC D BAHRIA TOWN LAHORE</h3>
      <h3>PH# 03234645823</h3>
<table class="table align-middle">
  <thead class="list-group mt-5 align-middle ">
    <tr class="list-group-item d-flex justify-content-around align-middle">
          <th> <span>Sr#</span></th>
          <th><span>Product</span></th>
          <th><span>Quantity</span></th>
          <th><span>Price</span></th>
        </li>
    </tr>
    </thead>
    <tbody class="list-group mt-5 align-middle">
      <% let total_price = 0 %>
      <% let date = new Date() %>
      <% list.forEach(list=>{ %>
      <tr class="list-group-item d-flex justify-content-around align-middle">
          <td><span><%= list.invoice_num %></span></td>
          <td><span><%= list.orderdetails %></span></td>
          <td><span><%= list.quantity %></span></td>
          <td><span><%= list.price %>rs</span></td>
        <% total_price+=list.price %>
    </tr>
    <% }) %>
    </tbody>
    </table>
    <div class="d-flex justify-content-around">
      <span class="m-5">Total Price: <%= total_price %>rs</span>
      <span class="m-5">Date: <%= date.toDateString() %></span>
      
    </div>
    
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>

    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
      integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
